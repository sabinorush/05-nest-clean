{
  "extends": [
    "@rocketseat/eslint-config/node"
  ],
  "rules": {
    "no-useless-constructor": "off",
    "no-self-assign": "off",
    "accessor-pairs": "off",
    "no-new": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "endOfLine": "auto"
        
      }
    ]
  }
}